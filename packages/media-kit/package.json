{
    "name": "@napgram/media-kit",
    "version": "0.1.4",
    "description": "Media conversion utilities for NapGram",
    "main": "./src/index.ts",
    "types": "./dist/src/index.d.ts",
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/src/index.d.ts",
            "import": "./src/index.ts",
            "default": "./src/index.ts"
        },
        "./package.json": "./package.json"
    },
    "scripts": {
        "build": "tsc -p tsconfig.json",
        "dev": "tsc -p tsconfig.json --watch"
    },
    "keywords": [
        "napgram",
        "media",
        "encoding"
    ],
    "author": "NapGram Team",
    "license": "MIT",
    "dependencies": {
        "@napgram/infra-kit": "workspace:*",
        "file-type": "^21.2.0",
        "image-js": "^1.3.0",
        "silk-wasm": "^3.7.1"
    },
    "devDependencies": {
        "@types/node": "25.0.3",
        "typescript": "5.9.3",
        "vitest": "4.0.16"
    },
    "publishConfig": {
        "main": "./dist/src/index.js",
        "types": "./dist/src/index.d.ts",
        "exports": {
            ".": {
                "types": "./dist/src/index.d.ts",
                "import": "./dist/src/index.js",
                "default": "./dist/src/index.js"
            },
            "./package.json": "./package.json"
        }
    }
}
